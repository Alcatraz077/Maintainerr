{
    "type": "sqlite",
    "database": "maintainerr.sqlite",
    "entities": [
        "dist/**/*.entity{.ts,.js}"
    ],
    "synchronize": true,
    "migrationsTableName": "migrations",
    "migrations": [
        "dist/database/migrations/*.js"
    ],
    "cli": {
        "migrationsDir": "src/database/migrations"
    }
}