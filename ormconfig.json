{
    "type": "sqlite",
    "database": "./data/maintainerr.sqlite",
    "entities": [
        "./server/dist/**/*.entities{.ts,.js}"
    ],
    "synchronize": false,
    "migrationsTableName": "migrations",
    "migrations": [
        "./server/dist/database/migrations/**/*{.js,.ts}"
    ],
    "cli": {
        "migrationsDir": "server/src/database/migrations"
    }
}